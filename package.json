  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run copy-assets&&rollup -c rollup.config.mjs&&node ./utils/replaceVersion.mjs",
    "build:dev": "npm run copy-assets&&rollup -c rollup.develop.config.mjs&&node ./utils/replaceVersion.mjs",
    "watch": "rollup -c rollup.develop.config.mjs -w",
    "copy-assets": "npx rimraf dist&&copyfiles -u 1 \"assets/**/*\" dist/",
    "check-types": "npx tsc --noEmit",
    "eslint": "eslint src/**/*.ts*",
    "eslint:fix": "eslint src/**/*.ts* --fix",
    "format": "prettier --write \"src/**/*.{tsx,ts,js,json,css,scss}\"",
    "lint": "npm run eslint&&npm run check-types",
    "lint:fix": "npm run eslint:fix&&npm run check-types&&npm run format",
    "prepare": "husky install",
    "dev": "vite"
  },
