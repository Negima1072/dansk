{
  "name": "DanSukuMizu-react",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "DanSukuMizu modified by Negima1072 and @eneko0513",
  "content_scripts": [
    {
      "matches": ["https://www.nicovideo.jp/watch/*"],
      "js": ["index.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["tabs"],
  "web_accessible_resources": [
    {
      "resources": ["main.js"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["img/*.png", "img/etc/*.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "64": "icon/icon64.png"
  },
  "action": {
    "default_icon": "icon/icon16.png",
    "default_title": "だんスク",
    "default_popup": "popup/popup.html"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  }
}
