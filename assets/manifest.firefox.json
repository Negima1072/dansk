{
  "name": "DanSukuMizu",
  "version": "@version",
  "manifest_version": 3,
  "description": "Modified By @Negima1072, @eneko0513 and @xpadev-net",
  "content_scripts": [
    {
      "matches": ["https://www.nicovideo.jp/watch/*"],
      "js": ["index.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["tabs"],
  "web_accessible_resources": [
    {
      "resources": ["main.js"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["img/*.png", "img/etc/*.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "64": "icon/icon64.png"
  },
  "action": {
    "default_icon": "icon/icon16.png",
    "default_title": "だんスク",
    "default_popup": "popup/popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{781bb7bf-090f-df5d-4976-67b3b958c4fe}"
    }
  }
}
